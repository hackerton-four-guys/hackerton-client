{
  "name": "minority-code",
  "displayName": "Minority Code",
  "description": "Validate python code by own code convention",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "require",
          "title": "Perfect Coop",
          "icon": "resources/handshake.svg"
        }
      ]
    },
    "views": {
      "require": [
				{
					"id": "require",
					"name": "Require"
				}
			]
    },
    "commands": [
      {
        "command": "minority-code.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "minority-code.refresh",
        "title": "Refresh"
      },
      {
        "command": "minority-code.printReferences",
        "title": "Print Reference"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.78.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "@vscode/test-electron": "^2.3.0"
  }
}
